	"main": "index.js",
	"scripts": {
		"start": "webpack serve --env PORT=3000",
		"build:prod": "npx webpack --env MODE=production",
		"build:dev": "npx webpack --env MODE=development",
		"test:unit": "jest --config ./configs/jest/jest.config.ts",
		"test:ui": "npx loki test",
		"test:ui:ok": "npx loki approve",
		"test:ui:report": "node scripts/generate-visual-json-report.js && npx reg-cli --from .loki/report.json --report .loki/report.html",
		"lint": "npx eslint src",
		"lint:ts": "eslint \"**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
		"lint:scss": "npx stylelint \"**/*.scss\"",
		"lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
		"storybook": "storybook dev -p 6006 -c ./configs/storybook",
		"storybook:build": "storybook build -c ./configs/storybook"
	},
